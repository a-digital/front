{
    "name": "adigital/front",
    "description": "Integration with Front",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "front"
    ],
    "support": {
        "docs": "https://github.com/a-digital/front/blob/master/README.md",
        "issues": "https://github.com/a-digital/front/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "A Digital",
            "homepage": "https://adigital.agency"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "craftcms/redactor": "^1.0.0-beta.1"
    },
    "autoload": {
        "psr-4": {
          "adigital\\front\\": "src/"
        }
    },
    "extra": {
        "name": "Front",
        "handle": "front",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/a-digital/front/master/CHANGELOG.md",
        "components": {
            "conversation": "adigital\\front\\services\\Conversation"
        },
        "class": "adigital\\front\\Front"
    }
}
